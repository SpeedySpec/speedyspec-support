{
    "name": "speedyspec/speedyspec-support",
    "version": "1.0.1",
    "description": "Common Classes used by SpeedySpec Packages",
    "type": "library",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=8.4"
    },
    "require-dev": {
        "pestphp/pest": "^3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "SpeedySpec\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SpeedySpec\\Support\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
